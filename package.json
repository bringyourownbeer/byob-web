{
	"name": "svelte-kit-blog-demo",
	"version": "0.0.1",
	"scripts": {
		"dev": "run-p netlify:dev svelte:dev --print-label",
		"svelte:dev": "svelte-kit dev --host",
		"netlify:dev": "npx netlify-cms-proxy-server",
		"build": "rm -rf build && svelte-kit build && touch .svelte-kit/static/build/.nojekyll",
		"start": "svelte-kit start",
		"deploy": "npm run build && npx gh-pages -d .svelte-kit/static/build -t true"
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^1.0.0-next.32",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.20",
		"@sveltejs/kit": "next",
		"dayjs": "^1.10.7",
		"js-yaml": "^3.14.1",
		"npm-run-all": "^4.1.5",
		"rehype-highlight": "^4.1.0",
		"rehype-stringify": "^8.0.0",
		"remark-frontmatter": "^3.0.0",
		"remark-gfm": "^1.0.0",
		"remark-parse": "^9.0.0",
		"remark-rehype": "^8.0.0",
		"sass": "^1.42.1",
		"svelte": "^3.43.1",
		"svelte-preprocess": "^4.9.8",
		"to-vfile": "^6.1.0",
		"unified": "^9.2.1",
		"vite": "^2.6.3"
	},
	"type": "module",
	"dependencies": {}
}
